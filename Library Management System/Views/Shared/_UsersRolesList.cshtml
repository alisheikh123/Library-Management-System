@model IEnumerable<Library_Management_System.Models.LR_AssignRolestoUsers>

@foreach (var item in Model)
{
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.user)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.category.name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.category.priority)
    </td>
    <td>
        @Html.ActionLink("Edit", "EditRoles", new { id = item.id }, htmlAttributes: new { @class = "btn btn-default" }) |
        @Html.ActionLink("Details", "DetailsRoles", new { id = item.id }, htmlAttributes: new { @class = "btn btn-primary" }) |
        @Html.ActionLink("Delete", "DeleteRoles", new { id = item.id }, htmlAttributes: new { @class = "btn btn-danger" })
    </td>
</tr>
}